{
  "extends": "airbnb-base",
  "parser": "@babel/eslint-parser",
  "rules": {
    "prefer-destructuring":"off",
    "no-param-reassign": "off",
    "import/no-cycle": "off",
    "max-len": [
      "error",
      120
    ],
    "linebreak-style": "off"
  },
  "plugins": [
    "babel"
  ]
}
